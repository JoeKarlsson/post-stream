{
  "name": "postStream",
  "author": "Jacoby Young, Ray Farias and Joe Karlsson",
  "version": "1.0.0",
  "engines": {
    "node": "6.2.0"
  },
  "description": "PostStream is an undesigned social network. It is a text-based response to the majority of modern social media platforms. This is certainly not for everyone as you will be interacting with others exclusively through text posts. You will notice several features intentionally left out in order to promote meaningful engagement. We hope you enjoy something a little different!",
  "keywords": [
    "social network",
    "Text",
    "PostStream",
    "Post",
    "Stream",
    "Social Media",
    "meaningful engagement",
    "Minimal",
    "undesigned",
    "text-based",
    "redux",
    "react-redux",
    "markdown",
    "text",
    "react",
    "express"
  ],
  "license": "MIT",
  "homepage": "https://poststream.herokuapp.com/",
  "scripts": {
    "start": "node server/server.js",
    "test": "./node_modules/.bin/mocha -R spec tests/server/**/**.spec.js",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./server/webpack/webpack.config.prod.js --progress --profile --colors",
    "postinstall": "rimraf dist && set NODE_ENV=production && webpack --config ./server/webpack/webpack.config.prod.js --progress --profile --colors"
  },
  "bugs": {
    "url": "https://github.com/JoeKarlsson/post-stream/issues"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeKarlsson/post-stream"
  },
